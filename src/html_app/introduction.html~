$('[data-role=page]').live('pageshow', function (event, ui) {

	// Turn off any embedded audio that might still be playing from the audio tag of another page
	try {
		var audioElement = document.getElementsByTagName('audio'); 
		audioElement.pause();
	} catch(err) {
		
	}

});

